{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "Bundler",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "types": ["vite/client", "node", "react", "react-dom"]
    },
    "include": [
      "src/renderer/**/*",
      "src/browser/**/*"
    ],
    "exclude": ["dist", "node_modules"]
  }